{
  "name": "nolimits3d-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Sito web professionale per NoLimits3D - Servizio di stampa 3D",
  "keywords": ["stampa-3d", "prototipi", "react", "typescript", "vite"],
  "author": "NoLimits3D",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "optimize-images": "imagemin public/images/*.{jpg,png,gif} --out-dir=public/images --plugin=imagemin-mozjpeg --plugin=imagemin-pngquant",
    "audit": "npm audit --audit-level=moderate",
    "test": "echo 'No tests specified' && exit 0"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-medium-image-zoom": "^5.1.10",
    "react-intersection-observer": "^9.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^0.19.2",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}